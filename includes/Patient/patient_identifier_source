<span class="p">};</span>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          

<span class="nx">fetch</span><span class="p">(</span><span class="dl">"</span><span class="s2">/openmrs/ws/rest/v1/idgen/identifiersource/:uuid</span><span class="dl">"</span><span class="p">,</span> <span class="nx">requestOptions</span><span class="p">)</span>                                                                                                                                                                                                                                                                                                                                                                                                                                        
  <span class="p">.</span><span class="nx">then</span><span class="p">(</span><span class="nx">response</span> <span class="o">=&gt;</span> <span class="nx">response</span><span class="p">.</span><span class="nx">text</span><span class="p">())</span>                                                                                                                                                                                                                                                                                                                                                                                                                                                        
