  <span class="o">.</span><span class="na">url</span><span class="o">(</span><span class="s">"/openmrs/ws/rest/v1/idgen/identifiersource/:uuid"</span><span class="o">)</span>                                                                                                                                                                                                                                                                                                                                                                                                                                                       
