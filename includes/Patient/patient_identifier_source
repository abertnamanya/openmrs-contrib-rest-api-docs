<span class="kd">var</span> <span class="nx">raw</span> <span class="o">=</span> <span class="nx">JSON</span><span class="p">.</span><span class="nx">stringify</span><span class="p">({</span><span class="dl">"</span><span class="s2">reservedIdentifiers</span><span class="dl">"</span><span class="p">:</span> <span class="dl">"</span><span class="s2">10501A,10502A,10503A,10504A,10505A</span><span class="dl">"</span><span class="p">});</span>                                                                                                                                                        

<span class="kd">var</span> <span class="nx">requestOptions</span> <span class="o">=</span> <span class="p">{</span>                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
  <span class="na">method</span><span class="p">:</span> <span class="dl">'</span><span class="s1">POST</span><span class="dl">'</span><span class="p">,</span>                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
